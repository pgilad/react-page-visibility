{
  "env": {
    "development": {
      "presets": [["es2015", { "modules": false }], "react"]
    },
    "production": {
      "presets": [["es2015", { "modules": false }], "react"],
      "plugins": [
        [
          "transform-react-remove-prop-types", {
            "ignoreFilenames": ["node_modules"],
            "mode": "wrap"
          }
        ]
      ]
    },
    "test": {
      "presets": ["es2015", "react"]
    }
  }
}
