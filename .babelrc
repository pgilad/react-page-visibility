{
    "env": {
        "production": {
            "presets": [["es2015", { "modules": "umd" }], "react"],
            "plugins": [
                [
                    "transform-react-remove-prop-types", {
                        "mode": "wrap"
                    }
                ]
            ]
        },
        "test": {
            "presets": ["es2015", "react"]
        }
    }
}
