    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "faucet": "0.0.1",
    "jsdom": "^9.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "tape": "^4.5.1",
    "tape-run": "^2.1.3",
    "webpack": "^2.6.1",
    "zuul": "^3.10.1"
  },
